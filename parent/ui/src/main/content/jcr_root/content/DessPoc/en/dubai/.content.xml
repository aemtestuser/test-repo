            <generic_text_0
                jcr:created="{Date}2015-01-08T13:09:19.151+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T13:20:12.022+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>&lt;b>The Dubai Mall&lt;/b>&amp;nbsp;is the world's largest shopping mall&amp;nbsp;based on total area and fourteenth largest by gross leasable area.Located in Dubai,United Arab Emirates&amp;nbsp;(U.A.E), it is part of the 20-billion-dollar Downtown Dubai&amp;nbsp;complex, and includes 1,200 shops.&lt;/p>&#xa;"
                textIsRich="true"/>
        </centre3>
