            <generic_text
                jcr:created="{Date}2015-01-08T13:01:46.835+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T13:03:46.292+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>&lt;b>Burj Khalifa&lt;/b>, known as&amp;nbsp;&lt;b>Burj Dubai&lt;/b>&amp;nbsp;prior to its inauguration, is a scyscraper&amp;nbsp;in Dubai, United Arab Emirates.&amp;nbsp;It is the tallest artificial structure in the world, standing at 829.8&amp;nbsp;m (2,722&amp;nbsp;ft).&lt;/p>&#xa;"
                textIsRich="true"/>
            <image_0
                jcr:created="{Date}2015-01-08T13:08:30.564+05:30"
