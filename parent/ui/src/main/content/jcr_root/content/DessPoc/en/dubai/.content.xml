            <generic_text
                jcr:created="{Date}2015-01-08T13:01:51.801+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T13:06:57.917+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>The&amp;nbsp;&lt;b>Palm Islands&lt;/b>&amp;nbsp;are two artificial islands, Palm Jumeirah&amp;nbsp;and Palm Jebel Ali, on the coast of Dubai, United Arab Emirates.&amp;nbsp;Palm Jumeirah takes the form of a palm tree, topped by a crescent.&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
            <image_0
                jcr:created="{Date}2015-01-08T13:08:34.030+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T13:11:49.282+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/image"
                height="220"
                imageRotate="0"
                width="200">
                <file/>
            </image_0>
            <generic_text_0
                jcr:created="{Date}2015-01-08T13:09:15.471+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T13:19:04.982+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>This&amp;nbsp;&lt;b>Crescent Moon Tower&lt;/b>&amp;nbsp;boasts a 33-storey down-turned half moon on the banks of the Caspian Sea. It is designed to accommodate a children's library amongst other attractions.&lt;/p>&#xa;"
                textIsRich="true"/>
        </centre2>
