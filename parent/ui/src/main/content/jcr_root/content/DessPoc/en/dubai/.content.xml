                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>&lt;b>Dubai Aquarium&lt;/b> is one of the largest indoor aquariums in the world and holds the Guinness World Record for the ‘The Single Largest Acrylic Panel'.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
