            <generic_text
                jcr:created="{Date}2015-01-08T12:51:48.372+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T12:52:00.209+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>&lt;b>Dubai&lt;/b>&amp;nbsp;(&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English&quot; title=&quot;Help:IPA for English&quot;>/&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English#Key&quot; title=&quot;Help:IPA for English&quot;>d&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English#Key&quot; title=&quot;Help:IPA for English&quot;>uː&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English#Key&quot; title=&quot;Help:IPA for English&quot;>ˈ&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English#Key&quot; title=&quot;Help:IPA for English&quot;>b&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English#Key&quot; title=&quot;Help:IPA for English&quot;>aɪ&lt;/a>&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_English&quot; title=&quot;Help:IPA for English&quot;>/&lt;/a>&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Wikipedia:Pronunciation_respelling_key&quot; title=&quot;Wikipedia:Pronunciation respelling key&quot;>&lt;i>doo-&lt;b>by&lt;/b>&lt;/i>&lt;/a>;&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Arabic_language&quot; title=&quot;Arabic language&quot;>Arabic&lt;/a>:&amp;nbsp;دبيّ‎&amp;nbsp;&lt;i>Dubayy&lt;/i>,&amp;nbsp;IPA:&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Help:IPA_for_Arabic&quot; title=&quot;Help:IPA for Arabic&quot;>[dʊˈbæj]&lt;/a>) is the&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/List_of_cities_in_the_United_Arab_Emirates#Major_cities&quot; title=&quot;List of cities in the United Arab Emirates&quot;>most populous&lt;/a>&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/City&quot; title=&quot;City&quot;>city&lt;/a>&amp;nbsp;and&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Emirates_of_the_United_Arab_Emirates&quot; title=&quot;Emirates of the United Arab Emirates&quot;>emirate&lt;/a>&amp;nbsp;in the&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/United_Arab_Emirates&quot; title=&quot;United Arab Emirates&quot;>United Arab Emirates&lt;/a>&amp;nbsp;(UAE), and the second largest emirate by territorial size after the capital,&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Abu_Dhabi_(emirate)&quot; title=&quot;Abu Dhabi (emirate)&quot;>Abu Dhabi&lt;/a>.&lt;/p>&#xa;"
                textIsRich="true"/>
        </wideSlider>
        <SearchRight
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
