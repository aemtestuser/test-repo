<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection"/>
            </tab1>
            <image1
                jcr:primaryType="cq:Widget"
                cropParameter="./image1/imageCrop"
                ddGroups="[media]"
                fieldLabel="Image One"
                fileNameParameter="./image1/fileName"
                fileReferenceParameter="./image1/fileReference"
                mapParameter="./image1/imageMap"
                name="./image1/file"
                rotateParameter="./image1/imageRotate"
                title="Image 1"
                xtype="html5smartimage">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image1/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </image1>
            <image2
                jcr:primaryType="cq:Widget"
                cropParameter="./image2/imageCrop"
                ddGroups="[media]"
                fieldLabel="Image One"
                fileNameParameter="./image2/fileName"
                fileReferenceParameter="./image2/fileReference"
                mapParameter="./image2/imageMap"
                name="./image2/file"
                rotateParameter="./image2/imageRotate"
                title="Image 2"
                xtype="html5smartimage">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image2/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </image2>
            <image3
                jcr:primaryType="cq:Widget"
                cropParameter="./image3/imageCrop"
                ddGroups="[media]"
                fileNameParameter="./image3/fileName"
                fileReferenceParameter="./image3/fileReference"
                mapParameter="./image3/imageMap"
                name="./image3/file"
                requestSuffix="/image3.img.png"
                rotateParameter="./image3/imageRotate"
                sizeLimit="100"
                title="Image 3"
                xtype="html5smartimage">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image3/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </image3>
            <image4
                jcr:primaryType="cq:Widget"
                cropParameter="./image4/imageCrop"
                ddGroups="[media]"
                fieldLabel="Image One"
                fileNameParameter="./image4/fileName"
                fileReferenceParameter="./image4/fileReference"
                mapParameter="./image4/imageMap"
                name="./image4/file"
                rotateParameter="./image4/imageRotate"
                title="Image 4"
                xtype="html5smartimage">
                <items jcr:primaryType="cq:WidgetCollection">
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image4/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </image4>
        </items>
    </items>
</jcr:root>
