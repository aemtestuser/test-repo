            <generic_text
                jcr:created="{Date}2015-01-07T17:27:45.072+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-07T18:17:55.398+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>&lt;b>Fes el Bali&lt;/b>&amp;nbsp;is the oldest walled part of&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Fes&quot; title=&quot;Fes&quot;>Fes&lt;/a>,&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Morocco&quot; title=&quot;Morocco&quot;>Morocco&lt;/a>. Fes el Bali was originally the capital of the&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/Idrisid_dynasty&quot; title=&quot;Idrisid dynasty&quot;>Idrisid dynasty&lt;/a>.&amp;nbsp;Besides being famous for having the&amp;nbsp;&lt;a href=&quot;http://en.wikipedia.org/wiki/University_of_Al-Karaouine&quot; title=&quot;University of Al-Karaouine&quot;>oldest university in the world&lt;/a>.&amp;nbsp;It is also believed to be the biggest car-free urban area in the world.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
            <image_0
                jcr:created="{Date}2015-01-07T18:12:34.968+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-07T18:13:35.292+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/image"
                height="220"
                imageRotate="0"
                width="210">
                <file/>
            </image_0>
            <generic_text_0
                jcr:created="{Date}2015-01-08T11:35:50.686+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T11:36:48.656+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>The magnificient ornate architecture of Fez is truly a sight,one which every lover of history should experience at least once in their lifetime.&lt;/p>&#xa;"
                textIsRich="true"/>
        </centre3>
