            <generic_text
                jcr:created="{Date}2015-01-07T17:22:44.894+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-07T18:17:01.261+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>The&amp;nbsp;&lt;b>Royal Palace&lt;/b>&amp;nbsp;comprises of 80 hectares of land and the Palace compound includes beautiful gardens, mosques and also an ancient Madrassa or a school for Qu’ranic studies that whose origin can be traced back to 14th century.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
            <image_0
                jcr:created="{Date}2015-01-07T18:09:57.566+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-07T18:10:28.562+05:30"
