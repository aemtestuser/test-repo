                jcr:lastModified="{Date}2015-01-07T18:18:07.521+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>Most of the city’s traditional crafts, such as &lt;b>leatherwork&lt;/b> and &lt;b>pottery&lt;/b> making, are practiced in the narrow, winding streets of the old city and are sold in that section’s traditional marketplaces, or&amp;nbsp;&lt;b>&lt;i>sūq&lt;/i>s&lt;/b>.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
            <image_0
                jcr:created="{Date}2015-01-07T18:11:28.596+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-07T18:12:02.286+05:30"
                jcr:lastModifiedBy="admin"
