            </image_0>
            <generic_text_0
                jcr:created="{Date}2015-01-08T11:33:38.525+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T11:35:35.843+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>Enjoy a subtle taste of Moroccan cuisine, while looking at the picturesque architecture of Fez, one of the most historical cities in the world&amp;nbsp;&lt;/p>&#xa;"
                textIsRich="true"/>
        </centre2>
        <centre3
