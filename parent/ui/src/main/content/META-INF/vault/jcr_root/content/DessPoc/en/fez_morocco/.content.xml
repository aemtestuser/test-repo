            <generic_text_0
                jcr:created="{Date}2015-01-08T11:29:23.976+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2015-01-08T11:32:11.257+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="acs-commons/components/content/generic-text"
                text="&lt;p>A panoramic view of Moulay Idriss Zerhoun, a town in Northern Morocco, is spread over two hills at the base of Mount Zerhoun. This holy land holds a special place in the hearts of the Moroccan people.&lt;/p>&#xa;"
                textIsRich="true"/>
        </centre>
        <wideSlider
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <image
                jcr:created="{Date}2015-01-07T16:43:43.039+05:30"
